package helloWorld;

import mdd_types;

enumeration Gender
        male, female
end;

class Dog
	attribute gender: Gender;
	attribute name: String;
end;

class Person
	attribute name: String := "Fido";
	operation adopt(p: Person, d: Dog): Ownership;
	
	operation setName();
	begin
		self.name := "foo";
	end;
end;

association Ownership
  navigable role owner : Person[1];
  navigable role owns : Dog[*];   
end;

end.